<module>
    <inherits name="com.google.gwt.user.User" />
    <inherits name="com.google.gwt.json.JSON" />

    <inherits name="org.cesiumjs.CesiumNoScript"/>
    <inherits name="org.openlayers.OpenLayersNoScript"/>
    <inherits name="org.ol3cesium.OLCesiumScript"/>

    <entry-point class="org.ol3cesium.olcs.OLCesiumEntryPoint" />

    <define-linker name="olCesiumScriptInjector" class="org.ol3cesium.linker.OLCesiumScriptInjector" />
    <add-linker    name="olCesiumScriptInjector"/>

    <set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>

    <source path="olcs"/>
</module>